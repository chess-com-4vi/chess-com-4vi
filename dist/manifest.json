{
  "manifest_version": 3,
  "name": "Chess-com-4vi",
  "description": "Adds accessibility for visually impaired players to Chess.com",
  "version": "1.0",
  "icons": {
    "16": "./assets/images/icon-16.png",
    "48": "./assets/images/icon-48.png",
    "128": "./assets/images/icon-128.png"
  },
  "content_scripts": [{
    "matches": ["*://www.chess.com/play/online*"],
    "run_at": "document_idle",
    "js": ["main.js"],
    "css": ["main.css"]
  }]
}